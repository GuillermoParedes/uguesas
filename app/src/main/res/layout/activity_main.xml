<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:weightSum="1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="HAMBURGUESAS UNIVERSITARIAST"
        android:id="@+id/textView"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="by Guillermo"
        android:id="@+id/textView2" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="20sp"
            android:id="@+id/b1"
            android:layout_gravity="left|top"
            android:background="@drawable/hnormal"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Fideitos Bs.15 "
            android:textSize="12dp"
            android:id="@+id/textView3"
            android:layout_gravity="left|top" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="20dp"
            android:id="@+id/b2"
            android:layout_gravity="center_horizontal|top"
            android:background="@drawable/hconhuevo"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Huevos Bs.13.5 "
            android:textSize="12dp"
            android:id="@+id/textView4"
            android:layout_gravity="center_horizontal|top" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="20dp"
            android:id="@+id/b3"
            android:layout_gravity="right|top"
            android:background="@drawable/hdoble"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H. Doble Bs 18"
            android:textSize="12dp"
            android:id="@+id/textView5"
            android:layout_gravity="right|top" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="-20dp"
            android:id="@+id/b4"
            android:layout_gravity="left|center_vertical"
            android:background="@drawable/hgemelas"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="GemelasBs.21"
            android:textSize="12dp"
            android:id="@+id/textView6"
            android:layout_gravity="left|center_vertical" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="-20dp"
            android:id="@+id/b5"
            android:layout_gravity="center"
            android:background="@drawable/hmariajuana"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="MariajuanaBs.25"
            android:textSize="12dp"
            android:id="@+id/textView7"
            android:layout_gravity="center" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="-20dp"
            android:id="@+id/b6"
            android:layout_gravity="right|center_vertical"
            android:background="@drawable/hmediokilo"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H. 1/2 Bs.18"
            android:textSize="12dp"
            android:id="@+id/textView8"
            android:layout_gravity="right|center_vertical" />



        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:layout_marginTop="340sp"
            android:id="@+id/b7"
            android:layout_gravity="left|top"
            android:background="@drawable/hconfideitos"
            android:onClick="mostrarHambur"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="NormalBs.12"
            android:textSize="12dp"
            android:id="@+id/textView9"
            android:layout_gravity="left|bottom" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:id="@+id/b8"
            android:layout_marginTop="340sp"
            android:layout_gravity="center_horizontal|top"
            android:background="@drawable/hpollo"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H.PolloBs12"
            android:textSize="12dp"
            android:id="@+id/textView10"
            android:layout_gravity="center_horizontal|bottom" />

        <ImageButton
            android:layout_width="100sp"
            android:layout_height="100sp"
            android:id="@+id/b9"
            android:layout_marginTop="340sp"
            android:layout_gravity="right|top"
            android:background="@drawable/hvegetal"
            android:onClick="mostrarHambur"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="VegetalBs.15"
            android:textSize="12dp"
            android:id="@+id/textView11"
            android:layout_gravity="right|bottom" />


    </FrameLayout>

</LinearLayout>
