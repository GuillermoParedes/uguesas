<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:weightSum="1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="HAMBURGUESAS UNIVERSITARIAS"
        android:id="@+id/textView"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="by Guillermo"
        android:id="@+id/textView2" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton"
            android:layout_gravity="left|top"
            android:background="@drawable/hconfideitos"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Fideitos Bs.15 "
            android:id="@+id/textView3"
            android:layout_gravity="left|top" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton2"
            android:layout_gravity="center_horizontal|top"
            android:background="@drawable/hconhuevo"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Huevos Bs.13.5 "
            android:id="@+id/textView4"
            android:layout_gravity="center_horizontal|top" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton3"
            android:layout_gravity="right|top"
            android:background="@drawable/hdoble"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H. Doble Bs 18"
            android:id="@+id/textView5"
            android:layout_gravity="right|top" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton4"
            android:layout_gravity="left|center_vertical"
            android:background="@drawable/hgemelas"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="GemelasBs.21"
            android:id="@+id/textView6"
            android:layout_gravity="left|center_vertical" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton5"
            android:layout_gravity="center"
            android:background="@drawable/hmariajuana"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="MariajuanaBs.25"
            android:id="@+id/textView7"
            android:layout_gravity="center" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton6"
            android:layout_gravity="right|center_vertical"
            android:background="@drawable/hmediokilo"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H. 1/2 Bs.18"
            android:id="@+id/textView8"
            android:layout_gravity="right|center_vertical" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton7"
            android:layout_gravity="left|bottom"
            android:background="@drawable/hnormal"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="NormalBs.12"
            android:id="@+id/textView9"
            android:layout_gravity="left|bottom" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton8"
            android:layout_gravity="center_horizontal|bottom"
            android:background="@drawable/hpollo"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H.PolloBs12"
            android:id="@+id/textView10"
            android:layout_gravity="center_horizontal|bottom" />

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageButton9"
            android:layout_gravity="right|bottom"
            android:background="@drawable/hvegetal"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="VegetalBs.15"
            android:id="@+id/textView11"
            android:layout_gravity="right|bottom" />


    </FrameLayout>

</LinearLayout>
